{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://your-backend-domain.onrender.com",
    "NEXT_PUBLIC_FRONTEND_URL": "https://your-frontend-domain.vercel.app"
  }
}